pipeline {
    agent any

    stages {

        stage ('Compile Stage')
        {
           steps{
            withMaven(maven : 'maven'){
                sh 'mvn clean compile'
            }
         } 
        } 
        stages('Testing Stage')
        {
            steps{
            withMaven(maven : 'maven'){
                sh 'mvn test'
            }
         } 

        }
        stages('Deployment  Stage')
        {
            steps{
            withMaven(maven : 'maven'){
                sh 'mvn deploy'
            }
         } 

        }
   
    }



}